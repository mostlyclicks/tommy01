<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(bio) -%>">
  <span class='title'>
    <%= bio.name %>

  </span>
  <span class='actions'>
    
    <%= link_to refinery_icon_tag("application_go.png"), refinery.bios_bio_path(bio),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    
    <%= link_to refinery_icon_tag("application_edit.png"), refinery.edit_bios_admin_bio_path(bio),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), refinery.bios_admin_bio_path(bio),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :confirm => t('message', :scope => 'refinery.admin.delete', :title => bio.name),
        :method => :delete %>
  </span>
</li>
